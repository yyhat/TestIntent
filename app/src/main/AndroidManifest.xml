<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hat.testintent" >

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityOther"/>
        <activity android:name=".ActivityFirst"/>
        <activity android:name="com.hat.testjson.ActivityDomXml"/>
        <activity android:name="com.hat.testjson.ActivitySaxXml"/>
        <activity android:name="com.hat.testjson.ActivityPullXml"/>
        <activity android:name="com.hat.testjson.ActivityJson"/>
        <activity android:name="com.hat.testjson.ActivityGson"/>
    </application>

</manifest>
